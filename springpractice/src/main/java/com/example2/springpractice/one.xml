<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"

xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">



<bean id="pt1" class="com.example2.springpractice.Point">

<property name="x" value="10"/>

<property name="y" value="20"/>

</bean>


<bean id="pt2" class="com.example2.springpractice.Point">

<property name="x" value="30"/>

<property name="y" value="40"/>

</bean>


<bean id="sg1" class="com.example2.springpractice.Point">

<property name="e1" ref="pt1"/>

<property name="e2" ref="pt2"/>

</bean>

</beans>